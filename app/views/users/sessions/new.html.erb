
<div class="container-form">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="container-box center">
      <div class="col-box">
        
        <h2>Acceso socio CUA</h2>
        

        <%= f.label :email, "Correo" %>
    
        <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "su@correo.cl" %>
        
        <%= f.label :password, "ContraseÃ±a" %>
        
        <%= f.password_field :password, autocomplete: "off" %>
            
        <% if devise_mapping.rememberable? -%>
          <div class="remember">
            <%= f.check_box :remember_me %>
            <%= f.label :remember_me, "Recordar" %>
          </div>
        <% end -%>
        
        <%= f.submit "Entrar", class: "btn btn-primary btn-block" %>
        
        
      </div>
    </div>
  <% end %>
</div>
<%= render "users/shared/links" %>
